<h1>Admin::Users#index</h1>
<p>Find me in app/views/admin/users/index.html.erb</p>

<h1>ユーザー一覧</h1>
<table>
	<thead>
		<tr>
		<th>プロフィール画像</th>
		<th>性</th>
		<th>名</th>
		<th>ユーザーネーム</th>
		<th>メールアドレス</th>
		<th>レベル</th>
		<th>ステータス</th>
		<th></th>
	</tr>
</thead>
<tbody>
<% @users.each do |user| %>
<tr>
<td><%= attachment_image_tag user, :user_image, fallback: "no-image.png", :size => "70x70" %></td>
<td><%= user.last_name %></td>
<td><%= user.first_name %></td>
<td><%= user.user_name %></td>
<td><%= user.email %></td>
<td><%= user.level %></td>
<td><%= user.is_active %></td>
<td><%= link_to "編集する", edit_admin_user_path(user) %></td>
</tr>
<% end %>

</tbody>
</table>